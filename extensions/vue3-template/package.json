{
  "$schema": "./@types/schema/package/index.json",
  "package_version": 2,
  "name": "vue3-template",
  "version": "1.0.0",
  "type": "module",
  "author": "Cocos Creator",
  "editor": ">=3.8.3",
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc && vite build",
    "build:watch": "concurrently \"yarn vue-tsc --watch\" \"yarn vite build --watch\""
  },
  "description": "i18n:vue3-template.description",
  "main": "./dist/main.js",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "path": "^0.12.7",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@cocos/creator-types": "^3.8.3",
    "@types/fs-extra": "^9.0.5",
    "@types/node": "^18.17.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "concurrently": "^8.2.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vue-tsc": "^2.0.29"
  },
  "panels": {
    "default": {
      "title": "vue3-template Default Panel",
      "type": "dockable",
      "main": "dist/panels/defaultPanel",
      "size": {
        "min-width": 400,
        "min-height": 300,
        "width": 1024,
        "height": 600
      }
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "i18n:menu.panel/vue3-template",
        "label": "i18n:vue3-template.open_panel",
        "message": "open-panel"
      },
      {
        "path": "i18n:menu.develop/vue3-template",
        "label": "i18n:vue3-template.send_to_panel",
        "message": "send-to-panel"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "openPanel"
        ]
      },
      "send-to-panel": {
        "methods": [
          "default.hello"
        ]
      }
    }
  }
}
