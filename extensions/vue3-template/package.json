{
  "$schema": "./@types/schema/package/index.json",
  "package_version": 2,
  "name": "vue3-template",
  "version": "1.0.0",
  "type": "module",
  "author": "Cocos Creator",
  "editor": ">=3.8.3",
  "license": "MIT",
  "engines": {
    "node": ">=22.8.0",
    "yarn": "1.x"
  },
  "scripts": {
    "preinstall": "node ./scripts/preinstall.js",
    "build": "vue-tsc && vite build --mode development",
    "build:watch": "concurrently \"yarn vue-tsc --watch\" \"yarn vite build --watch --mode development\"",
    "build:production": "vue-tsc && vite build --mode production"
  },
  "description": "i18n:vue3-template.description",
  "main": "./dist/main.js",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "path": "^0.12.7",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@cocos/creator-types": "^3.8.3",
    "@types/fs-extra": "^9.0.5",
    "@types/node": "^22.5.5",
    "@vitejs/plugin-vue": "^5.1.2",
    "concurrently": "^8.2.2",
    "javascript-obfuscator": "^4.1.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "terser": "^5.31.6",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vue-tsc": "^2.0.29"
  },
  "panels": {
    "default": {
      "title": "vue3-template Default Panel",
      "type": "dockable",
      "main": "dist/panels/defaultPanel",
      "size": {
        "min-width": 400,
        "min-height": 300,
        "width": 1024,
        "height": 600
      }
    }
  },
  "contributions": {
    "menu": [
      {
        "path": "i18n:menu.panel/vue3-template",
        "label": "i18n:vue3-template.open_panel",
        "message": "open-panel"
      },
      {
        "path": "i18n:menu.develop/vue3-template",
        "label": "i18n:vue3-template.send_to_panel",
        "message": "send-to-panel"
      }
    ],
    "messages": {
      "open-panel": {
        "methods": [
          "openPanel"
        ]
      },
      "send-to-panel": {
        "methods": [
          "default.hello"
        ]
      }
    },
    "shortcuts": [
      {
        "message": "open-panel",
        "win": "ctrl+o",
        "mac": "cmd+o"
      }
    ]
  }
}
